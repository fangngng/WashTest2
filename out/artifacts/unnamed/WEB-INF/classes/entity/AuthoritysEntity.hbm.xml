<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.AuthoritysEntity" table="Authoritys" schema="dbo" catalog="WashDB">
        <id name="authId" column="AuthID"/>
        <property name="authName" column="AuthName"/>
        <property name="authPath" column="AuthPath"/>
        <property name="authUrl" column="AuthUrl"/>
        <property name="parentId" column="ParentID"/>
        <property name="depth" column="Depth"/>
        <property name="isActive" column="IsActive"/>
        <property name="createTime" column="CreateTime"/>
    </class>
</hibernate-mapping>