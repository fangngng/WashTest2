<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.OrderInfoEntity" table="OrderInfo" schema="dbo" catalog="WashRoom">
        <property name="orderId" column="OrderID"/>
        <property name="orderCreateDate" column="OrderCreateDate"/>
        <property name="userId" column="UserID"/>
        <property name="orderEndDate" column="OrderEndDate"/>
        <property name="orderStatus" column="OrderStatus"/>
        <property name="orderIsPayed" column="OrderIsPayed"/>
        <property name="orderMoney" column="OrderMoney"/>
        <property name="orderPayedMoney" column="OrderPayedMoney"/>
        <property name="orderCoupons" column="OrderCoupons"/>
        <property name="payMethod" column="PayMethod"/>
        <property name="isActive" column="IsActive"/>
        <property name="createTime" column="CreateTime"/>
    </class>
</hibernate-mapping>