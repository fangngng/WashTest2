<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.LoginUserEntity" table="LoginUser" schema="dbo" catalog="WashRoom">
        <property name="loginId" column="LoginID"/>
        <property name="loginName" column="LoginName"/>
        <property name="loginPwd" column="LoginPwd"/>
        <property name="isActive" column="IsActive"/>
        <property name="lastLogintime" column="LastLogintime"/>
        <property name="loginCount" column="LoginCount"/>
        <property name="createTime" column="CreateTime"/>
        <property name="createBy" column="CreateBy"/>
    </class>
</hibernate-mapping>