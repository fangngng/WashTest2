<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.UserInfoEntity" table="UserInfo" schema="dbo" catalog="WashRoom">
        <property name="userId" column="UserID"/>
        <property name="userName" column="UserName"/>
        <property name="userPhone" column="UserPhone"/>
        <property name="userAddr" column="UserAddr"/>
        <property name="userGroup" column="UserGroup"/>
        <property name="userSex" column="UserSex"/>
        <property name="isActive" column="IsActive"/>
        <property name="createTime" column="CreateTime"/>
    </class>
</hibernate-mapping>